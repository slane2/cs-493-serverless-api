    },
    "HellurLambdaVersionQ0Em73fd8htj8LHzE7JU1SZ87LLjyxuNvhT5iUhh0": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
