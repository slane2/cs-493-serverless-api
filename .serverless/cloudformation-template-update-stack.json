          },
          "S3Key": "serverless/music-api/dev/1614224285980-2021-02-25T03:38:05.980Z/music-api.zip"
        },
        "Handler": "handler_py.hellur",
        "Runtime": "python3.8",
        "FunctionName": "music-api-dev-hellur",
        "MemorySize": 1024,
        "Timeout": 6,
        "Environment": {
          "Variables": {
            "SERVERLESS_EXPRESS_PLATFORM": "aws"
          }
        },
        "Role": {
          "Fn::GetAtt": [
            "IamRoleLambdaExecution",
            "Arn"
